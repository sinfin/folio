= index_header

= catalogue(@versions)
  ruby:
    attribute :key

    attribute :updated_at do
      l(record.updated_at, format: "%-d. %-m. %Y, %-H:%M:%S")
    end

    actions({ invalidate: {
      name: :invalidate,
      icon: :reload,
      method: :post,
      confirm: true,
      url: -> (record) { url_for([:invalidate, :console, record]) }
    } }, :destroy)
