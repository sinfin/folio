.<%= classname_prefix %>-atom.<%= classname_prefix %>-atom-embed-video.my-atom-lg
  .container-fluid
    - if options[:console_preview]
      .p-5.text-center.font-weight-bold.bg-gray
        = model.class.model_name.human
    - else
      == cell('<%= application_namespace_path %>/ui/embed',
              model.embed_code,
              caption: model.caption)
