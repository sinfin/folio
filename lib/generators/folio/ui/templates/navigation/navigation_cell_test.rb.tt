# frozen_string_literal: true

require "test_helper"

class <%= application_namespace %>::Ui::NavigationCellTest < Cell::TestCase
  test "show" do
    create_and_host_site

    menu = <%= application_namespace %>::Menu::Header.create!(locale: :en, site: @site)

    html = cell("<%= application_namespace_path %>/ui/navigation", menu).(:show)
    assert html.has_css?(".<%= classname_prefix %>-ui-navigation")
  end
end
